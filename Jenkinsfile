pipeline {
      agent any
      stages {
               stage('Build') {
                steps {
                    echo 'hello 1'
               }
             }
               stage('Test') {
                  steps {
                       sleep 45
                    }
            }
              stage('Deploy') {
                   steps {
                           echo 'hello 1'
                    }
           } 
     }                           
 }

